package br.mil.eb.decex.sisaluno.enumerated;

public enum Linha {
	
	BELICA("Bélica"),
	SAUDE("Saúde"),
	CIENTIFICA("Científica"),
	COMPLEMENTAR("Complementar");
	
	
	private String descricao;
	
	Linha(String descricao) {
		this.descricao = descricao;
	}

	public String getDescricao() {
		return descricao;
	}

}
