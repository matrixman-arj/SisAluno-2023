package br.mil.eb.decex.sisaluno.enumerated;

public enum TipoVinculo {
	// Tipo de formação que o aluno está realizando.
	
	OTT("Oficial Técnico Temporário"),
	R2("Oficial da Reserva não remunerada"),
	STT("Sargento Técnico Temporário");
	
	
	private String descricao;
	
	TipoVinculo(String descricao) {
		this.descricao = descricao;
	}

	public String getDescricao() {
		return descricao;
	}
}
